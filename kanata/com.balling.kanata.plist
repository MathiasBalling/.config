<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.balling.kanata</string>
    <key>ProgramArguments</key>
    <array>
        <string>sudo</string>
        <string>/Users/balling/.cargo/bin/kanata</string>
        <string>-c</string>
        <string>/Users/balling/.config/kanata/config.kbd</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <dict>
      <key>SuccessfulExit</key>
      <false/>
      <key>Crashed</key>
      <true/>
    </dict>
    <key>StandardErrorPath</key>
    <string>/Users/balling/.config/kanata/error.log</string>
    <key>ProcessType</key>
    <string>Interactive</string>  
    <key>Nice</key>
    <integer>-20</integer>
</dict>
</plist>
